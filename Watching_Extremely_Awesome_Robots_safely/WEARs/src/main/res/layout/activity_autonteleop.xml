                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="4dp"
                        android:layout_weight="2"
                        android:background="@color/white"
                        android:gravity="left|center"
                        android:text="High"
                        android:textSize="25dp"
                        tools:ignore="HardcodedText,NestedWeights,RtlHardcoded,SpUsage" />

                    <Button
                        android:id="@+id/tHtHSub"
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:background="@color/lightBlue"
                        android:onClick="setTHtHSub"
                        android:text="-1"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        tools:ignore="HardcodedText,SpUsage" />

                    <TextView
                        android:id="@+id/tHtHDs"
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="4dp"
                        android:layout_weight="1"
                        android:background="#D7DDEA"
                        android:gravity="center"
                        android:text="0"
                        android:textSize="32dp"
                        tools:ignore="HardcodedText,SpUsage" />

                    <Button
                        android:id="@+id/tHtHAdd"
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:background="@color/lightBlue"
                        android:onClick="setTHtHAdd"
                        android:text="+1"
                        android:textColor="@color/white"
                        android:textSize="15dp"
                        tools:ignore="HardcodedText,SpUsage" />


                </LinearLayout>

            </LinearLayout> <!--Teleop Hatch-->

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/background_material"
                tools:ignore="ContentDescription" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners_chunk_material"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingTop="10dp">


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:paddingLeft="16dp"
                        android:paddingRight="15dp"
                        android:paddingTop="5dp"
                        android:text="Endgame"
                        android:textColor="@color/textMedium"
                        android:textSize="30dp"
                        android:textStyle="italic"
                        tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp"
                    tools:ignore="RtlHardcoded,RtlSymmetry">

                    <RadioGroup
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:checkedButton="@id/onField">

                        <RadioButton
                            android:id="@+id/onField"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="12dp"
                            android:layout_marginTop="8dp"
                            android:theme="@style/myRadioButtonStyle"
                            android:onClick="setOnField"
                            android:text="Not In Hab Zone"
                            android:textColor="@color/textMedium"
                            android:textSize="26dp"
                            tools:ignore="HardcodedText,SpUsage" />

                        <RadioButton
                            android:id="@+id/lvlOne"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="12dp"
                            android:theme="@style/myRadioButtonStyle"
                            android:onClick="setLvlOne"
                            android:text="Level 1"
                            android:textColor="@color/textMedium"
                            android:textSize="26dp"
                            tools:ignore="HardcodedText,SpUsage" />

                        <RadioButton
                            android:id="@+id/lvlTwo"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="12dp"
                            android:theme="@style/myRadioButtonStyle"
                            android:onClick="setLvlTwo"
                            android:text="Level 2"
                            android:textColor="@color/textMedium"
                            android:textSize="26dp"
                            tools:ignore="HardcodedText,SpUsage" />

                        <RadioButton
                            android:id="@+id/lvlThree"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="12dp"
                            android:theme="@style/myRadioButtonStyle"
                            android:onClick="setLvlThree"
                            android:text="Level 3"
                            android:textColor="@color/textMedium"
                            android:textSize="26dp"
                            tools:ignore="HardcodedText,SpUsage" />


                    </RadioGroup>

                </LinearLayout>

            </LinearLayout> <!--Teleop Endgame-->

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:background="@color/background_material"
                tools:ignore="ContentDescription" />

            <LinearLayout
                android:id="@+id/tNext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners_chunk_material"
                android:orientation="horizontal"
                android:paddingBottom="10dp"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="10dp">

                <Button
                    android:id="@+id/gotoPostMatch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/lightBlue"
                    android:onClick="setGoToPostMatch"
                    android:text="Next"
                    android:textColor="@color/white"
                    tools:ignore="HardcodedText" />

            </LinearLayout> <!--Next-->

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="8dp"
                android:background="@color/background_material"
                tools:ignore="ContentDescription" />

        </LinearLayout>

    </ScrollView>

</RelativeLayout>

